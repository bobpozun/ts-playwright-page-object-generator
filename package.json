{
  "name": "ts-playwright-page-object-generator",
  "version": "1.0.1",
  "description": "A powerful tool for automatically generating Playwright Page Objects from web pages. Focuses on interactive elements by default and generates clean, maintainable TypeScript code.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "playwright test --project=local-tests",
    "test:live": "playwright test --project=live-tests",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "playwright",
    "page-object",
    "test-automation",
    "web-testing",
    "accessibility",
    "typescript",
    "e2e-testing",
    "test-generation",
    "webdriver",
    "ui-testing"
  ],
  "author": "Robert Pozun <robert.pozun@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobpozun/ts-playwright-page-object-generator.git"
  },
  "bugs": {
    "url": "https://github.com/bobpozun/ts-playwright-page-object-generator/issues"
  },
  "homepage": "https://github.com/bobpozun/ts-playwright-page-object-generator#readme",
  "peerDependencies": {
    "@playwright/test": ">=1.20.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^8.0.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
